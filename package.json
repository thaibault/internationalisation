{
    "name": "language",
    "version": "1.0.0",
    "description": "Expand your audience",
    "keywords": ["lang", "language", "internationalisation"],
    "files": ["index.js"],
    "repository": {
        "type": "git", "url": "https://github.com/thaibault/language"
    },
    "homepage": "http://torben.website/language",
    "main": "index.compiled.js",
    "webOptimizer": {
        "injection": {
            "externalAliases": {"tools": "$.Tools"},
            "internal": {"vendor": {
                "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['buildDLL', 'serve', 'testInBrowser', 'watchDLL'].includes(self.givenCommandLineArguments[2]) ? ['tools'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['qunitjs', 'webOptimizer/browserAPI']) : []"
            }}
        },
        "module": {
            "optimizer": {"uglifyJS": null},
            "skipParseRegularExpression": {
                "__evaluate__": "/\\/(?:(?:jquery)|(?:module)|([^/]+\\.min))\\.js(?:\\?.+|$)/"
            }
        }
    },
    "scripts": {
        "build": "webOptimizer build",
        "buildDLL": "webOptimizer buildDLL",
        "clear": "webOptimizer clear",
        "document": "webOptimizer document",
        "lint": "webOptimizer typeCheck && webOptimizer lint",
        "postinstall": "webOptimizer build",
        "preinstall": "webOptimizer preinstall",
        "serve": "webOptimizer serve",
        "start": "npm run serve",
        "test": "webOptimizer test",
        "testInBrowser": "webOptimizer testInBrowser",
        "watch": "webOptimizer build --watch",
        "watchDLL": "webOptimizer buildDLL --watch"
    },
    "license": "CC-BY-3.0",
    "dependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git",
        "tools": "git+ssh://git@github.com/thaibault/tools.git"
    },
    "devDependencies": {
        "jsdom": ">=9.4.1",
        "qunitjs": ">=2.0.0",
        "qunit-cli": ">=0.2.0*",
        "source-map-support": ">=0.4.2"
    },
    "peerDependencies": {"jquery": ">=3.1.0"}
}
