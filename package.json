{
  "name": "internationalisation",
  "version": "1.0.243",
  "license": "CC-BY-3.0",
  "description": "Expand your audience",
  "homepage": "https://torben.website/internationalisation",
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS",
      "frFR"
    ],
    "tagline": "Expand your audience<!--deDE:Erweitern Sie Ihr Publikum--><!--frFR:Ã‰largir votre audience-->",
    "trackingCode": "UA-40192634-6"
  },
  "keywords": [
    "lang",
    "language",
    "internationalisation"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "type.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/internationalisation"
  },
  "scripts": {
    "build": "weboptimizer build",
    "build:export": "weboptimizer build '{__reference__: \"target:export\"}'",
    "check": "yarn check:types; yarn lint",
    "check:types": "weboptimizer check:types",
    "clear": "weboptimizer clear",
    "document": "weboptimizer document",
    "lint": "weboptimizer lint",
    "prepare": "yarn build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "weboptimizer serve",
    "start": "yarn serve",
    "test": "weboptimizer test",
    "test:browser": "weboptimizer test:browser",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "weboptimizer build --watch"
  },
  "devDependencies": {
    "@babel/eslint-parser": "*",
    "@babel/plugin-transform-proto-to-assign": "*",
    "@types/ejs": "*",
    "@types/html-minifier": "*",
    "@types/jest": "*",
    "@types/jquery": "*",
    "@types/jsdom": "*",
    "@types/node": "*",
    "@types/rimraf": "*",
    "@types/workbox-webpack-plugin": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "clientnode": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
    "jest": "*",
    "jquery": "*",
    "jsdoc": "*",
    "weboptimizer": "*",
    "webpack-dev-server": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "clientnode": "*",
    "jquery": "*"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "jquery": {
      "optional": true
    }
  },
  "resolutions": {
    "@types/webpack": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz"
  },
  "sideEffects": false,
  "webOptimizer": {
    "commandLine": {
      "test": {
        "environments": [
          "jsdom",
          "node"
        ]
      }
    },
    "#": "Only reasonable if have a linked clientnode repository next to this one and updates their should be propagated directly in development mode.",
    "development": {
      "server": {
        "contentBase": {
          "__append__": {
            "__evaluate__": "path.resolve(self.path.base, '../clientnode')"
          }
        }
      }
    },
    "target:export": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "@babel/plugin-transform-runtime"
              }
            }
          }
        }
      }
    },
    "webpack": {
      "watchOptions": {
        "ignored": {
          "__prepend__": [
            "**/node_modules/**/node_modules",
            "**/node_modules/**/*.jsx",
            "**/node_modules/**/*.ts",
            "**/node_modules/**/*.tsx"
          ],
          "__remove__": "**/node_modules"
        }
      }
    }
  },
  "dependencies": {
    "@types/webpack": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz"
  }
}
