  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "jquery": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10",
    "yarn": ">=4"
  },
  "resolutions": {
    "colors": "1.4.0"
  },
  "sideEffects": false,
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS",
      "frFR"
    ],
    "tagline": "Expand your audience<!--deDE:Erweitern Sie Ihr Publikum--><!--frFR:Ã‰largir votre audience-->",
    "trackingCode": "UA-40192634-6"
  },
  "webOptimizer": {
    "commandLine": {
      "test": {
        "environments": [
          "jsdom"
        ]
      }
    },
    "#": "Only reasonable if have a linked clientnode repository next to this one and updates their should be propagated directly in development mode.",
    "development": {
      "server": {
        "contentBase": {
          "__append__": {
            "__evaluate__": "path.resolve(self.path.base, '../clientnode')"
          }
        }
      }
    },
    "target:bundle": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "@babel/plugin-transform-runtime"
              }
            }
          }
        }
      }
    },
    "webpack": {
      "watchOptions": {
        "ignored": {
          "__prepend__": [
            "**/node_modules/**/node_modules",
            "**/node_modules/**/*.jsx",
            "**/node_modules/**/*.ts",
            "**/node_modules/**/*.tsx"
          ],
          "__remove__": "**/node_modules"
        }
      }
    }
  },
  "packageManager": "yarn@4.1.0+sha256.81a00df816059803e6b5148acf03ce313cad36b7f6e5af6efa040a15981a6ffb"
}
