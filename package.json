{
  "name": "internationalisation",
  "version": "1.0.70",
  "license": "CC-BY-3.0",
  "description": "Expand your audience",
  "homepage": "http://torben.website/internationalisation",
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS",
      "frFR"
    ],
    "tagline": "Expand your audience<!--deDE:Erweitern Sie Ihr Publikum--><!--frFR:Ã‰largir votre audience-->",
    "trackingCode": "UA-40192634-6"
  },
  "keywords": [
    "lang",
    "language",
    "internationalisation"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/internationalisation"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "build:export": "webOptimizer build '{__reference__: \"export\"}'",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "webOptimizer serve",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:browser": "webOptimizer test:browser",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "clientnode": "*",
    "jquery": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "clientnode": "^3.0.15",
    "coveralls": "^3.0.0",
    "documentation-website": "^1.0.116",
    "eslint": "^4.19.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jsdoc": "^3.5.0",
    "flow-bin": "^0.68.0",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.3",
    "nyc": "^11.6.0",
    "qunit": "^2.5.1",
    "source-map-support": "^0.5.3",
    "weboptimizer": "^2.0.45",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "webOptimizer": {
    "export": {
      "development": {
        "tool": false
      },
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "transform-runtime"
              }
            }
          }
        }
      }
    },
    "files": {
      "compose": {
        "cascadingStyleSheet": null
      }
    },
    "injection": {
      "external": {
        "implicit": {
          "pattern": {
            "exclude": {
              "__append__": {
                "__evaluate__": "/^clientnode\\/test(?:\\.compiled)?(?:\\.js)?$/"
              }
            }
          }
        }
      },
      "internal": {
        "vendor": {
          "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['build:dll', 'serve', 'test:browser', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? ['babel-polyfill', 'clientnode'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['script!qunit', 'weboptimizer/browserAPI']) : []"
        }
      }
    },
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "mangle": false
          }
        }
      },
      "skipParseRegularExpressions": {
        "__evaluate__": "/(?:^|\\/)(?:(?:babel-polyfill\\/dist\\/polyfill)|(?:jquery\\/dist\\/jquery)|(?:webpack\\/buildin\\/global)|(?:webpack\\/buildin\\/module))(?:\\.min)?\\.js(?:\\?.+|$)/"
      }
    }
  }
}
