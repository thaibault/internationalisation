    "@typescript-eslint/parser": "^7.1.0",
    "clientnode": "^3.0.1136",
    "documentation-website": "^1.0.325",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^48.2.0",
    "favicons-webpack-plugin": "^6.0.1",
    "image-minimizer-webpack-plugin": "^4.0.0",
    "jest": "^29.7.0",
    "jquery": "^3.7.1",
    "jsdoc": "^4.0.2",
